package Exercico_113;

public class BubbleSort {

	public static void main(String[] args) {
		

		System.out.println();
	}
	
	public static void bubbleSort(int[] arry) {
		int tamnho = arry.length;
		
		
		for (int i = 0; i < tamnho - 1; i++) {
			for (int j = 0; j < tamnho - 1 - i; j++) {
				if (arry[j] > arry[j + 1]) {
					int temp = arry[j];
					arry[j] = arry[j + 1];
					arry[j + 1] = temp;
				}
			}
		}
	}

}
